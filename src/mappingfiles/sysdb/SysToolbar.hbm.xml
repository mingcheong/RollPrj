<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="mappingfiles.sysdb.SysToolbar" 
    table="SYS_TOOLBAR"
    lazy="false"
    dynamic-update="true"
    dynamic-insert="true"
>

    <id
        name="TOOLBAR_ID"
        type="java.lang.String"
        column="TOOLBAR_ID"
    >
        <generator class="assigned" />
    </id>



    <property
        name="TOOLBAR_CODE"
        type="java.lang.String"
        column="TOOLBAR_CODE"
        not-null="true"
        length="42"
    />
    <property
        name="TOOLBAR_NAME"
        type="java.lang.String"
        column="TOOLBAR_NAME"
        length="60"
    />
    <property
        name="TOOLBAR_TYPE"
        type="int"
        column="TOOLBAR_TYPE"
        not-null="true"
        length="2"
    />
    <property
        name="MENU_ID"
        type="java.lang.String"
        column="MENU_ID"
        length="38"
    />
    <property
        name="TOOLBAR_ICON"
        type="java.lang.String"
        column="TOOLBAR_ICON"
        length="100"
    />
    <property
        name="TIPS"
        type="java.lang.String"
        column="TIPS"
        length="100"
    />
    <property
        name="LAST_VER"
        type="java.lang.String"
        column="LAST_VER"
        length="30"
    />


    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to SysMenu -->
    
        
    
        
    

</class>
</hibernate-mapping>

