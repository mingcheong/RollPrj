<class 
    name="mappingfiles.sysdb.SysLog" 
    table="SYS_LOG"
    lazy="false"
    dynamic-update="true"
    dynamic-insert="true"
>

    <id
        name="LOG_TYPE"
        type="java.lang.String"
        column="LOG_TYPE"
    >
        <generator class="assigned" />
    </id>



    <property
        name="LOG_TYPENAME"
        type="java.lang.String"
        column="LOG_TYPENAME"
        length="60"
    />
    <property
        name="IS_DEFALUTLOG"
        type="java.lang.Integer"
        column="IS_DEFALUTLOG"
        length="1"
    />
    <property
        name="SEARCHLOGCLASS"
        type="java.lang.String"
        column="SEARCHLOGCLASS"
        length="100"
    />
    <property
        name="LAST_VER"
        type="java.lang.String"
        column="LAST_VER"
        length="30"
    />


    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to SysLoginfo -->
        
        
		
    
        
           
        
      
           
        
   

